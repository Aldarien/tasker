{
    "name": "aldarien/tasker",
    "description": "Task manager asociated with people, places, tasks, dates",
    "type": "project",
    "require": {
        "slim/slim": "^3.10",
        "rubellum/slim-blade-view": "^0.1.1",
        "aldarien/config": "^1.0",
        "aldarien/url": "^1.2",
        "aldarien/migrator": "dev-develop",
        "aldarien/paris": "dev-develop",
        "aldarien/language": "dev-develop",
        "aldarien/definitions": "dev-develop"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.0",
        "zeuxisoo/slim-whoops": "^0.6",
        "kint-php/kint": "^2.2"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Aldarien",
            "email": "aldarien85@gmail.com"
        }
    ],
    "autoload": {
      "psr-4": {
        "App\\": "app",
        "Tasker\\": "src"
      },
      "files": [
        "app/Helper/functions.php"
      ]
    },
    "repositories": [
      {
        "type": "path",
        "url": "../migrator"
      },
      {
        "type": "path",
        "url": "../paris"
      },
      {
        "type": "path",
        "url": "../language"
      },
      {
        "type": "path",
        "url": "../definitions"
      }
    ]
}
